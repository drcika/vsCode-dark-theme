{
  "name": "Cika Black",
  "type": "dark",
  "colors": {
    //
    "editor.background": "#000",
    "peekViewEditor.background": "#000",
    "editorGroupHeader.tabsBackground": "#000",
    "titleBar.activeBackground": "#000",
    "titleBar.inactiveBackground": "#000",
    "tab.inactiveBackground": "#000",
    "tab.border": "#000",
    "activityBar.background": "#000",
    "sideBar.background": "#000",
    "statusBar.background": "#000",
    "statusBar.noFolderBackground": "#000",
    "dropdown.background": "#000",
    "input.background": "#000",
    //
    "sideBar.border": "#111",
    "focusBorder": "#111",
    "dropdown.border": "#111",
    "list.inactiveSelectionBackground": "#111",
    "list.hoverBackground": "#111",
    "editorHoverWidget.background": "#111",
    "editorWidget.background": "#111",
    "editorSuggestWidget.background": "#111",
    "editorSuggestWidget.border": "#111",
    "editorHoverWidget.border": "#111",
    "activityBarBadge.background": "#111",
    //
    "editor.lineHighlightBackground": "#333",
    "editorIndentGuide.background": "#333",
    "editorGroup.border": "#333",
    "tab.activeBackground": "#333",
    "sideBarSectionHeader.background": "#333",
    "editorLineNumber.foreground": "#333",
    "scrollbarSlider.background": "#333",
    "scrollbarSlider.hoverBackground": "#333",
    "scrollbarSlider.activeBackground": "#333",
    "list.activeSelectionBackground": "#333",
    "tab.activeBorder": "#333",
    "panel.border": "#333",
    //
    "editorLineNumber.activeForeground": "#aaa",
    "progressBar.background": "#aaa",
    "selection.background": "#aaa",
    //
    "foreground": "#ddd",
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi",
        "string.quoted.docstring.multi storage.type.string",
      ],
      "settings": {
        "foreground": "#444444"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    // Typescript
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    // Typescript
    {
      "name": "Keywords",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.import"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "Punctuation Parameters",
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "entity.name.variable.parameter",
        "meta.at-rule.function variable",
        "meta.at-rule.mixin variable",
        "meta.function.arguments",
        "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql",
        "variable.parameter"
      ],
      "settings": {
        // "foreground": "#fff"
        "foreground": "#76b198"
      }
    },
    {
      "name": "Punctuation Template Expression",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        // "foreground": "#8df"
        "foreground": "#ff8888"
      }
    },
    {
      "name": "Storage Type Arrow Function",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        // "foreground": "#91DD64"
        "foreground": "#78f6ac"
      }
    },
    {
      "name": "String Template",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        // "foreground": "#91DD64"
        "foreground": "#78f6ac"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "Support Function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[JAVASCRIPT JSX] React Class",
      "scope": [
        "support.class.component.js"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        // "foreground": "#fff"
        "foreground": "#76b198"
      }
    },
    // CSS ----------------------------------------------------------------------
    {
      "name": "[CSS] - Entity",
      "scope": [
        "source.css",
        "source.stylus",
        "source.scss",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - Hex hex",
      "scope": [
        "punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - keys",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        // "foreground": "#fff"
        "foreground": "#769eb1"
      }
    },
    {
      "name": "[CSS] - Keyword",
      "scope": [
        "source.css punctuation.definition.keyword",
        "source.css keyword.control"
      ],
      "settings": {
        "foreground": "#f6b"
      }
    },
    {
      "name": "[CSS] - Important",
      "scope": "keyword.other.important.scss",
      "settings": {
        "foreground": "#f6b"
      }
    },
    {
      "name": "[CSS] - Punctiation",
      "scope": [
        "punctuation.definition.entity.css"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - ID hastags",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - support misc function",
      "scope": [
        "support.function.misc.scss"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - ID Selector",
      "scope": [
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#8df"
      }
    },
    {
      "name": "[CSS] - Element Selector",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - Support",
      "scope": [
        "source.css support",
        "source.stylus support",
        "source.css support.constant"
      ],
      "settings": {
        // "foreground": "#fff"
        "foreground": "#769eb1"
      }
    },
    {
      "name": "[CSS] - Constant",
      "scope": [
        "source.stylus constant",
        "source.css constant"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - support constant",
      "scope": [
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "[CSS] - units",
      "scope": [
        "keyword.other.unit.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.percentage.css",
        "constant.other.color.rgb-value.hex.css",
        "keyword.other.unit.ms.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vw.css",
        "keyword.other.unit.deg.css",
        "keyword.other.unit.fr.css"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - String",
      "scope": [
        "source.css string",
        "source.css punctuation.definition.string",
        "source.stylus string",
        "source.stylus punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#98EC65"
      }
    },
    {
      "name": "[CSS] - Variable",
      "scope": [
        "source.css variable",
        "source.stylus variable"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    // HTML ----------------------------------------------------------------------
    {
      "name": "[HTML] - Entity",
      "scope": [
        "entity.other"
      ],
      "settings": {
        "foreground": "#8df"
      }
    },
    {
      "name": "[HTML] - Elements",
      "scope": [
        "entity.name.tag.block.any.html",
        "entity.name.tag.inline.any.html",
        "entity.name.tag.structure.any.html",
        "entity.name.tag.html",
        "meta.jsx.children.tsx"
      ],
      "settings": {
        "foreground": "#00AAEF"
      }
    },
    {
      "name": "[HTML] - Keywords",
      "scope": [
        "punctuation.definition.keyword",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "[HTML] - ID value",
      "scope": "meta.toc-list.id.html",
      "settings": {
        // "foreground": "#91DD64"
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "[HTML] - Meta tags",
      "scope": [
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[HTML] - Tags",
      "scope": [
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.begin.html"
      ],
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "[HTML] - Text basic",
      "scope": "text.html.basic",
      "settings": {
        "foreground": "#c3c3c3"
      }
    },
    {
      "name": "[HTML] - Quotes",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
      "settings": {
        // "foreground": "#91DD64"
        "foreground": "#78f6ac"
      }
    },
    // Javascript ----------------------------------------------------------------------
    {
      "name": "[JAVASCRIPT] - Numbers",
      "scope": "source.js constant",
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[JAVASCRIPT] - Keywords",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#f6b"
      }
    },
    {
      "name": "[JAVASCRIPT] - Keywords",
      "scope": "source.js keyword",
      "settings": {
        "foreground": "#f6b"
      }
    },
    {
      "name": "[JAVASCRIPT] - Storage Type Function",
      "scope": "source.js storage.type.function",
      "settings": {
        // "foreground": "#fff"
        "foreground": "#769eb1"
      }
    },
    {
      "name": "[JAVASCRIPT] - Keywords",
      "scope": [
        "source.js punctuation.definition.keyword",
        "source.js keyword.control"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "[JAVASCRIPT] - Keywords",
      "scope": [
        "source.js punctuation.definition.keyword",
        "source.js keyword.control"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "[JAVASCRIPT] - Variable Language",
      "scope": "variable.language, entity.name.type.class.js, meta.tag.js, entity.name.tag.js",
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "[JAVASCRIPT] - Inherited Component",
      "scope": [
        "entity.other.inherited-class.js",
        "variable.language.this.js",
        "variable.other.readwrite.alias.js",
        "meta.import.js"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    // Lines
    {
      "name": "Meta Brace",
      "scope": [
        "meta.brace"
      ],
      "settings": {
        "foreground": "#888"
      }
    },
    // Typescript ----------------------------------------------------------------------
    {
      "name": "Support Variable Property DOM",
      "scope": "support.variable.property.dom",
      "settings": {
        "foreground": "#76b198"
      }
    },
    {
      "name": "[JSON] - Support",
      "scope": "source.json support",
      "settings": {
        // "foreground": "#fff"
        "foreground": "#769eb1"
      }
    },
    {
      "name": "[JSON] - String",
      "scope": [
        "source.json string",
        "source.json punctuation.definition.string",
        "punctuation.definition.string.end.json",
        "punctuation.definition.string.begin.json"
      ],
      "settings": {
        "foreground": "#78f6ac"
      }
    }
    // React -----------------------------------------------------------------------
  ]
}